<div class="container frontend">

<div class="row">
<div class="page-header">
  <h1>
    <%= render partial: 'shared/editable_span', locals: {label: 'about_title'} %>
    <small>
      <%= render partial: 'shared/editable_span', locals: {label: 'about_title2'} %>
    </small>
  </h1>
</div>

<!--Content-->
<div class="col-md-12">
  <div class="header-splash-img">
    <div class="header-splash-intro">
      <h1>
        <%= render partial: 'shared/editable_span', locals: {label: 'about_h1'} %>
      </h1>

      <%= render partial: 'shared/editable_p', locals: {label: 'about_main_text'} %>
    </div>
  </div>
</div>

<!--Skills and nums-->

<div class="block skills-block">
  <h2>
    <%= render partial: 'shared/editable_span', locals: {label: 'about_title3'} %>
  </h2>

  <div class="col-md-12">
    <div class="row">
      <% 1.upto 4 do |i| %>
          <div class="col-md-3 col-sm-6">
            <h3 class="text-center">
              <%= render partial: 'shared/editable_span', locals: {label: "about_value#{i}_title"} %>
            </h3>

            <div class="easy-pie-chart-container">
              <%= render partial: 'shared/editable_p', locals: {label: "about_value#{i}_text"} %>
            </div>
          </div>
      <% end %>

    </div>
    <div class="row">
      <h2>Les valeurs d'Activ'Action</h2>

      <div class="col-md-2 col-sm-4 col-xs-6">
        <div class="company-numbers"><i class="entypo-cog"></i>
          <h4>1 200 567</h4>

          <p>Pages Created</p>
        </div>
      </div>
      <div class="col-md-2 col-sm-4 col-xs-6">
        <div class="company-numbers"><i class="entypo-lifebuoy"></i>
          <h4>600 567</h4>

          <p>Tickets Resolved</p>
        </div>
      </div>
      <div class="col-md-2 col-sm-4 col-xs-6">
        <div class="company-numbers"><i class="entypo-hourglass"></i>
          <h4>1800</h4>

          <p>Hours per Project</p>
        </div>
      </div>
      <div class="col-md-2 col-sm-4 col-xs-6">
        <div class="company-numbers"><i class="entypo-comment"></i>
          <h4>200 567</h4>

          <p>Social Comments</p>
        </div>
      </div>
      <div class="col-md-2 col-sm-4 col-xs-6">
        <div class="company-numbers"><i class="entypo-thumbs-up"></i>
          <h4>100 349</h4>

          <p>Likes</p>
        </div>
      </div>
      <div class="col-md-2 col-sm-4 col-xs-6">
        <div class="company-numbers"><i class="entypo-trophy"></i>
          <h4>16</h4>

          <p>Awards Collected</p>
        </div>
      </div>
    </div>
  </div>
  <div class="clearfix"></div>
</div>

